{
  "name": "registers-specification",
  "description": "Registers Specification builder",
  "license": "MIT",
  "scripts": {
    "install-all": "yarn install && yarn install-bib && yarn install-hiblocks && yarn install-restdoc && yarn install-svg",
    "install-bib": "yarn --cwd 'gatsby-remark-bib' install",
    "install-hiblocks": "yarn --cwd 'gatsby-remark-hiblocks' install",
    "install-restdoc": "yarn --cwd 'gatsby-remark-restdoc' install",
    "install-svg": "yarn --cwd 'gatsby-remark-svg' install",
    "develop": "VERSION=$(git rev-parse --short HEAD) PUBDATE=$(git show -s --format=%cI) gatsby develop",
    "build": "yarn clean && VERSION=$(git rev-parse --short HEAD) PUBDATE=$(git show -s --format=%cI) gatsby build",
    "clean": "rm -rf .cache public",
    "deploy": "netlify deploy -d public -m",
    "serve": "gatsby serve",
    "test": "snyk test"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.10",
    "emotion": "^10.0.9",
    "emotion-server": "^10.0.9",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "gatsby": "^2.3.11",
    "gatsby-plugin-emotion": "^4.0.6",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-prismjs": "^3.2.6",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-toml": "^2.1.9",
    "gatsby-transformer-yaml": "^2.1.11",
    "github-slugger": "^1.2.1",
    "js-yaml": "^3.13.0",
    "netlify-cli": "^2.10.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19",
    "webpack": ">=4.4.0"
  },
  "devDependencies": {
    "snyk": "^1.135.1"
  }
}
